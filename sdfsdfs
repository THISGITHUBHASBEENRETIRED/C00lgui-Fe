b.Name = "Backing" 
b.Size = UDim2.new(0,20,0,100) 
b.Position = UDim2.new(0.5,-10,0.5,-50) 
b.BorderSizePixel = 0 
b.BackgroundColor3 = Color3.new(0.1,0.45,0.01) 
b.ZIndex = 1 
SPB = b 
