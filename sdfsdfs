local button = Instance.new("TextButton")
	button.Parent = psd
	button.BackgroundColor3 = blak
	button.BorderColor3 = rede
	button.BorderSizePixel = 3
	button.Name = "Empty"
	button.Position = UDim2.new(0,0,0,132)
	button.Size = UDim2.new(0.499,0,0,30)
	button.ZIndex = 2
	button.Font = tef
	button.FontSize = "Size14"
	button.Text = "Sing C00lkidd Theme"
	button.TextColor3 = whit
	button.TextWrapped = true
	button.MouseButton1Down:connect(function()
local song = "TXVHuWM4"

request = game:HttpGet("https://pastebin.com/raw/" .. song)

decoded = game.HttpService:JSONDecode(request)

local lyrics = {}
for i in decoded.lyrics:gmatch("[^\r\n]+") do
   table.insert(lyrics, i)
end

for i, v in pairs(lyrics) do
   wait(3)
   game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(v, "All")
end
		end)
