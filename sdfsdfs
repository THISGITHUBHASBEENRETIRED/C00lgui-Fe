if v:IsA("Tool") then
v.Parent = game.Players.LocalPlayer.Character
end
end
end)

TextButton_23.MouseButton1Click:connect(function()
Body_Axis_Rotation = TextBox.Text


_=wait
AnimationId="181525546"
A=Instance.new("Animation")
A.AnimationId="rbxassetid://"..AnimationId
k=game:GetService("Workspace")[game:GetService("Players").LocalPlayer.Name]:FindFirstChild("Humanoid"):LoadAnimation(A)
k:Play()
k:AdjustSpeed(1)
_(Body_Axis_Rotation*.010625)
k:AdjustSpeed(0)
end)

TextButton_24.MouseButton1Click:connect(function()
Float_Height = TextBox_2.Text

local i=1
G=game;
_=wait;
p=G:GetService("Players").LocalPlayer.Character
p:FindFirstChild("Humanoid").HipHeight = Float_Height
_(1.5);
t=.4;
n=2;
for i=1,i do
repeat p:FindFirstChild("Humanoid").HipHeight=Float_Height-n _(t)p:FindFirstChild("Humanoid").HipHeight=Float_Height+n _(t) until
p:FindFirstChild("Humanoid").Health == 0
end
end)

