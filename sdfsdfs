fr.Parent = b 
fr.Size = UDim2.new(1,0,0.5,0) 
fr.Position = UDim2.new(0,0,0.5,0) 
fr.BackgroundColor3 = Color3.new(0.1,0.65,0.01) 
SPF = fr 

