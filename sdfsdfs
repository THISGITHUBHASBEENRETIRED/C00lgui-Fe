HCol = "a"
LaserCol = "Really black"
LaserCol2 = "Really  red"
LaserCol3 = "Really black"
Posing = "Follow"
Facing =  "Owner"
Anim = {}
Anim.a = "None"
Anim.b = "None"
Anim.Welding =  0
local fingwide = 1.5*s
local finghei = 2.8
