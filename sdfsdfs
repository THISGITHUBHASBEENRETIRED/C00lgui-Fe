b2.Text = "0"
local prev2 = Instance.new("Frame", FontS2)
prev2.Size = UDim2.new(1,0,.5,0)
prev2.Position = UDim2.new(0,0,.5,0)
prev2.BackgroundColor3 = Color3.new(0,0,0)
prev2.BorderColor3 = Color3.new(0,0,0)
